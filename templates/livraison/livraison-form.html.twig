{% extends 'admin/NavDash.html.twig' %}
    {% block body %}
        <div style="display:flex;align-items: center;justify-content: center;margin-top: 150px;">
            <!-- jquery validation -->
            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">Modifier livraison   <small>Session Admin</small></h3>
                    {{ form_start(form,{attr:{novalidate:'novalidate'}}) }}
                </div>
                <!-- /.card-header -->
                <!-- form start -->
                <form id="quickForm" novalidate="novalidate">
                    <div class="card-body">
                        <div class="form-group">
                            <label for="exampleInputReference1">Reference</label>
                            {{ form_widget(form.ref,{'attr':{'class':'form-control'}}) }}
                            {{ form_errors(form.ref,{'attr':{'class':'form-control'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputLocalisation1">Localisation</label>
                            {{ form_widget(form.localisation,{'attr':{'class':'form-control'}}) }}
                            {{ form_errors(form.localisation,{'attr':{'class':'form-control'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEtat1">Etat</label>
                            {{ form_widget(form.etat,{'attr':{'class':'form-control'}}) }}
                            {{ form_errors(form.etat,{'attr':{'class':'form-control'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputUserID1">User ID</label>
                            {{ form_widget(form.user,{'attr':{'class':'form-control'}}) }}
                            {{ form_errors(form.user,{'attr':{'class':'form-control'}}) }}
                        </div>
                        <div class="form-group">
                            <label for="exampleInputUserID1">Date de Sortie</label>
                            {{ form_widget(form.datesortie,{'attr':{'class':'form-control'}}) }}
                            {{ form_errors(form.datesortie,{'attr':{'class':'form-control'}}) }}
                        </div>




                    </div>
                    <!-- /.card-body -->

                    {{ form_end(form) }}
                </form>
            </div>
            <!-- /.card -->
        </div>
    {% endblock %}