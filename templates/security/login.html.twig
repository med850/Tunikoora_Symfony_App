

<title>{% block title %}Inscription | Connexion{% endblock %}</title>
{% block stylesheets %}

 <link rel="stylesheet" href="{{asset("assets/css/loginFormDes.css")}}">
 
{% endblock %}

{% block body %}

<img class = "img"src="{{asset("adminAssets/img/tunikoora.png")}}" alt="Logo" height="120" width="120">
<div id="login-box">
  <div class="left">
    <h1>Connexion</h1>


<form method="post">
   

  
    <div class="form-group">
      <input type="text"  placeholder="E-mail" required  name="email" class="form-control" value="{{ last_username }}"id="inputEmail"/>
    </div>
    <div class="form-group">
    <input type="password" name="password" id="inputPassword" class="form-control" autocomplete="current-password" required placeholder="Mot de passe">
    </div>
    <input type="hidden" name="_csrf_token"
           value="{{ csrf_token('authenticate') }}" >

    {#
        Uncomment this section and add a remember_me option below your firewall to activate remember me functionality.
        See https://symfony.com/doc/current/security/remember_me.html

        <div class="checkbox mb-3">
            <label>
                <input type="checkbox" name="_remember_me"> Remember me
            </label>
        </div>
    #}

   <div class="form-group">
    <input type="submit" name="signup_submit" value="Connexion" />
    </div>
</form>
{% endblock %}
