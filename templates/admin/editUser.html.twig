
{% extends 'admin/NavDash.html.twig' %}
{% block title %}Modifier un utilisateur{% endblock %}

{% block body %}


 <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
             <nav class="navbar navbar-light bg-light">
        <form class="form-inline">
     </form>
    </nav>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Modifier un utilisateur</li>
             
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

  <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
            <div class="card">

            <div class="card card-primary">
              <div class="card-header">
                <h3 class="card-title">Quick Example</h3>
              </div>
              <!-- /.card-header -->
              <!-- form start -->
            
                <div class="card-body">
                  <div class="form-group">
                    <label >Cin</label><br>
                      {{ form_start(userForm, {'attr':{'novalidate':'novalidate'}}) }}
                    {{ form_widget(userForm.cin,{'attr':{'class': 'form-control'}}) }}
                    <div class="my-custom-class-for-errors">
                    {{ form_errors(userForm.cin) }}
                  </div>
                  </div>
                   <div class="form-group">
                    <label >Nom</label><br>
                      {{ form_start(userForm, {'attr':{'novalidate':'novalidate'}}) }}
                    {{ form_widget(userForm.username,{'attr':{'class': 'form-control'}}) }}
                    <div class="my-custom-class-for-errors">
                    {{ form_errors(userForm.username) }}
                  </div>
                  </div>
                  <div class="form-group">
                    <label >Prénom</label><br>
                      {{ form_start(userForm, {'attr':{'novalidate':'novalidate'}}) }}
                    {{ form_widget(userForm.prenom,{'attr':{'class': 'form-control'}}) }}
                    <div class="my-custom-class-for-errors">
                    {{ form_errors(userForm.prenom) }}
                  </div>
                  </div>

                    <div class="form-group">
                    <label >Tél</label><br>
                      {{ form_start(userForm, {'attr':{'novalidate':'novalidate'}}) }}
                    {{ form_widget(userForm.tel,{'attr':{'class': 'form-control'}}) }}
                    <div class="my-custom-class-for-errors">
                    {{ form_errors(userForm.tel) }}
                  </div>
                  </div>


                      <div class="form-group">
                    <label>Email</label><br>
                      {{ form_start(userForm, {'attr':{'novalidate':'novalidate'}}) }}
                    {{ form_widget(userForm.email,{'attr':{'class': 'form-control'}}) }}
                    <div class="my-custom-class-for-errors">
                    {{ form_errors(userForm.email) }}
                  </div>
                  </div>


                  <div class="form-check">
                     {{ form_start(userForm, {'attr':{'novalidate':'novalidate'}}) }}
                    {{ form_widget(userForm.email,{'attr':{'class': 'form-check-input'}}) }}

                   
                  </div>
                </div>
                <!-- /.card-body -->

               {{form_end(userForm) }}
            </div>


          <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
            <div class="card">
            </div>



            {% endblock %}